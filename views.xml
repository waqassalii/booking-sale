                                    <field name="new_booking_priority_id"
                                           attrs="{'invisible':[('new_booking_priority_id', '=', False)]}"/>
                                    <field name="new_mode_of_transport"
                                           attrs="{'invisible':[('new_mode_of_transport', '=', False)]}"/>
                                    <field name="new_train_type"
                                           attrs="{'invisible':[('new_train_type', '=', False)]}"/>
                                    <field name="new_booking_type_id"
                                           attrs="{'invisible':[('new_booking_type_id', '=', False)]}"/>
                                    <field name="new_cargo_type"
                                           attrs="{'invisible':[('new_cargo_type', '=', False)]}"/>
                                    <field name="new_commodity_id"
                                           attrs="{'invisible':[('new_commodity_id', '=', False)]}"/>
                                    <field name="new_invoicing_party_id"
                                           attrs="{'invisible':[('new_invoicing_party_id', '=', False)]}"/>
                                    <field name="new_owner_id" attrs="{'invisible':[('new_owner_id', '=', False)]}"/>
                                    <field name="new_start_date"
                                           attrs="{'invisible':[('new_start_date', '=', False)]}"/>
                                    <field name="new_vasel_cutoff_date"
                                           attrs="{'invisible':[('new_vasel_cutoff_date', '=', False)]}"/>
                                    <field name="new_pickup_date"
                                           attrs="{'invisible':[('new_pickup_date', '=', False)]}"/>
