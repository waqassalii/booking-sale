                                                    <field name="new_cargo_weightcont_in_tons"
                                                           attrs="{'invisible': [('new_cargo_weightcont_in_tons', '=', 0.0)]}"/>
                                                    <field name="new_cargo_weight_in_tons"
                                                           attrs="{'invisible': [('new_cargo_weightcont_in_tons', '=', 0.0)]}"/>
                                                    <field name="new_gross_cargo_weight_in_tons"
                                                           attrs="{'invisible': [('new_gross_cargo_weight_in_tons', '=', 0.0)]}"/>
                                                    <field name="new_bag_id"
                                                           attrs="{'invisible': [('new_bag_id', '=', False)]}"/>
                                                    <field name="new_net_bag_weight"
                                                           attrs="{'invisible': [('new_net_bag_weight', '=', 0.0)]}"/>
                                                    <field name="new_gross_bag_weight"
                                                           attrs="{'invisible': [('new_gross_bag_weight', '=', 0.0)]}"/>
                                                    <field name="new_bag_weight_type"
                                                           attrs="{'invisible': [('new_bag_weight_type', '=', False)]}"/>
                                                    <field name="new_bag_qty"
                                                           attrs="{'invisible': [('new_bag_qty', '=', 0.0)]}"/>
                                                    <field name="new_vehicle_id"
                                                           attrs="{'invisible': [('new_vehicle_id', '=', False)]}"/>
                                                    <field name="new_wheeler"
                                                           attrs="{'invisible': [('new_wheeler', '=', 0)]}"/>
                                                    <field name="new_vehicle_tare_weight"
                                                           attrs="{'invisible': [('new_vehicle_tare_weight', '=', 0.0)]}"/>
                                                    <field name="new_vehicle_qty"
                                                           attrs="{'invisible': [('new_vehicle_qty', '=', 0.0)]}"/>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
