                                                           attrs="{'invisible': [('rate_type', '!=', 'bag')]}"/>
                                                    <field name="bag_weight_type"
                                                           attrs="{'invisible': [('rate_type', '!=', 'bag')]}"/>
                                                    <field name="bag_qty"
                                                           attrs="{'invisible': [('rate_type', '!=', 'bag')]}"/>
                                                    <field name="vehicle_id"
                                                           attrs="{'invisible': [('rate_type', '!=', 'vehicle')]}"/>
                                                    <field name="wheeler"
                                                           attrs="{'invisible': [('rate_type', '!=', 'vehicle')]}"/>
                                                    <field name="vehicle_tare_weight"
                                                           attrs="{'invisible': [('rate_type', '!=', 'vehicle')]}"/>
                                                    <field name="vehicle_qty"
                                                           attrs="{'invisible': [('rate_type', '!=', 'vehicle')]}"/>
                                                </group>
                                                <group>
                                                    <field name="pil_weight"/>
                                                    <field name="desired_rate"/>
                                                    <field name="discount_rate" readonly="1"/>
                                                    <field name="discounted_rate" readonly="1"/>
                                                    <field name="final_rate" readonly="1"/>
                                                    <field name="total_amount" readonly="1"/>
                                                    <field name="booking_id" invisible="1"/>
                                                </group>
                                            </group>
                                        </sheet>
                                    </form>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- actions opening views on models -->
        <record model="ir.actions.act_window" id="booking_module_action_window">
            <field name="name">Booking</field>
            <field name="res_model">booking.module</field>
            <field name="view_mode">kanban,tree,form</field>
        </record>


        <!-- Top menu item -->
        <menuitem name="Booking" id="booking_module_menu_root"/>
        <!-- Child menu -->
        <menuitem name="Booking"
                  id="booking_module_parent_menu"
                  sequence="1"
                  parent="booking_module_menu_root"/>
        <menuitem name="Configurations"
                  id="booking_module_configuration_menu"
                  parent="booking_module_menu_root"
                  sequence="3"/>

        <!--        action menu-->
        <menuitem name="Booking"
                  id="booking_module_menu"
                  parent="booking_module_parent_menu"
                  sequence="1"
                  action="booking_module_action_window"/>
    </data>
</odoo>
