                                                               attrs="{'invisible': [('new_discounted_rate', '=', 0.0)]}"/>
                                                        <field name="new_final_rate" readonly="1"
                                                               attrs="{'invisible': [('new_final_rate', '=', 0.0)]}"/>
                                                        <field name="new_total_amount" readonly="1"
                                                               attrs="{'invisible': [('new_total_amount', '=', 0.0)]}"/>
                                                    </group>

                                                </group>
                                                <group>
                                                    <field name="new_rate_type"
                                                           attrs="{'invisible': [('new_rate_type', '=', False)]}"/>
                                                    <field name="new_container_size_id"
                                                           attrs="{'invisible': [('new_container_size_id', '=', False)]}"/>
                                                    <field name="new_container_qty"
                                                           attrs="{'invisible': [('new_container_qty', '=', 0.0)]}"/>
