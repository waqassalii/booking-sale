                                               attrs="{'invisible': [('rate_type', '!=', 'bag')]}"/>
                                        <field name="net_bag_weight"
                                               attrs="{'invisible': [('rate_type', '!=', 'bag')]}"/>
                                        <field name="gross_bag_weight"
