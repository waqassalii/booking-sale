                                </group>
                                <group>
                                    <field name="new_itinerary_id"
                                           attrs="{'invisible':[('new_itinerary_id', '=', False)]}"/>
                                    <field name="new_route_1" attrs="{'invisible':[('new_route_1', '=', False)]}"/>
                                    <field name="new_first_mile"
