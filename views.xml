                                    <field name="vasel_cutoff_date"
                                           attrs="{'invisible': [('export_import', '!=', 'export')]}"/>
                                    <field name="free_days"/>
                                </group>
                                <group>
                                    <field name="new_date" attrs="{'invisible':[('new_date', '=', False)]}"/>
